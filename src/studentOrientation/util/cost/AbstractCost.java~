//---------------------------------------------------------------------
package studentOrientation.util.cost;
//---------------------------------------------------------------------
import studentOrientation.orientation.interfaces.UnitsInterface;
//---------------------------------------------------------------------

public abstract class AbstractCost implements UnitsInterface
{
	Character unitChar;
	String text;

	public Character toChar()
	{
		return unitChar;
	}
	public String toText()
	{
		return text;
	}
}
