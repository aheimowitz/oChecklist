//---------------------------------------------------------------------
package studentOrientation.util;
//---------------------------------------------------------------------
public class Debug
{
	private int debugVal;

	/**
	*	The empty class constructor
	*
	*	Assumes the passed value is in
	*	 the correct range.
	**/
	Debug(int debugVal)
	{
		this.debugVal = debugVal;
	}

	/**
	*	This method returns the debug value
	*	@return Returns the current debug val
	**/
	public int getVal()
	{
		return debugVal;
	}

	/**
	*	This method sets the debug value
	**/
	public void setVal(int debugVal)
	{
		this.debugVal = debugVal;
	}
}
//---------------------------------------------------------------------
